<div class="container">
    <p class="resultTitle">Result Board</p>
<div class="table-container">

    <table id="data-table">
      <thead>
        <tr>
          <th>Exam ID</th>
          <th>Class</th>
          <th>Subject</th>
          <th>Chapter</th>
          <th>Marks</th>
          <th>View</th>
        </tr>
       
      </thead>
      <tbody id="data-body">
        <!-- Table rows will be added dynamically using JavaScript -->
        {{#each resultinfoStore}}
         <tr>
            <td>{{this._id}}</td>
            <td>{{this.class}}</td>
            <td>{{this.subject}}</td>
            <td>{{this.chapter}}</td>
            <td>{{this.result}}</td>
            <td><button  onclick="answerHistory('{{this._id}}')">View</button></td>
        </tr>
        {{/each}}
      </tbody>
    </table>
