<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <!-- bootstrap cdn links -->
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css"
      integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm"
      crossorigin="anonymous"
    />

    <!-- icon library -->
    <link
      href="https://unpkg.com/ionicons@4.5.10-0/dist/css/ionicons.min.css"
      rel="stylesheet"
    />
    <link rel="stylesheet" href="/css/exam.css" />
    <title>Document</title>
  </head>
  <body>
    <div class="container">
      <div class="container_wrapper">
        <div class="main_page">
          <div class="heading heading1">
            <div>
              <p class="title">ANSWER SHEET</p>

              {{!-- <div class="sub_title">{{chapterNumber}}</div> --}}
            </div>

            {{! <div class="card">
              <span id="countdown" style="font-weight: bold"></span>
            </div> }}
          </div>
          <!-- <hr class="hr_line" /> -->
          <div class="exam_information">
            <p class="instruction">
              If there have any doubt about the answer of result please kindy
              inform us with ss and share this in the support.
            </p>

            <div class="student_information">
              <form>
                {{! <label
                  for="email"
                  class="color hover-underline-animation"
                >Email</label>
                <br /> }}
                {{! <input type="email" name="email" />
                <br />
                <span>example@example.com</span> }}
                <hr />

                <div class="question_wrapper">
                  {{#each randomItems}}
                    <div class="single_question">
                      <p>Q{{inc @index}}. {{this.question}}</p>
                      <div class="four_option">
                        <div>
                          <label>
                            <input
                              type="radio"
                              name="{{this._id}}"
                              value="{{this.option1}}"
                            />
                            <span>{{this.option1}}</span>
                          </label>
                        </div>
                        <div>
                          <label>
                            <input
                              type="radio"
                              name="{{this._id}}"
                              value="{{this.option2}}"
                            />
                            <span>{{this.option2}}</span>
                          </label>
                        </div>
                        <div>
                          <label>
                            <input
                              type="radio"
                              name="{{this._id}}"
                              value="{{this.option3}}"
                            />
                            <span>{{this.option3}}</span>
                          </label>
                        </div>
                        <div>
                          <label>
                            <input
                              type="radio"
                              name="{{this._id}}"
                              value="{{this.option4}}"
                            />
                            <span>{{this.option4}}</span>
                          </label>
                        </div>
                      </div>
                    </div>
                    <div class="rightAns">
                      <p>Correct Answer: {{this.correct}}</p>
                    </div>
                  {{/each}}
                  {{#each polynomialMcq}}
                    <div class="single_question polynomial">

                      <p>Q25. {{this.question}}</p>
                      <div class="polynomial_data">
                        <ul>
                          <li>i) {{this.polynomial1}}</li>
                          <li>ii) {{this.polynomial2}}</li>
                          <li>iii) {{this.polynomial3}}</li>
                        </ul>
                        <p>&nbsp;&nbsp;Choose the correct answer</p>
                      </div>
                      <div class="four_option">
                        <div>
                          <label>
                            <input
                              type="radio"
                              name="{{this._id}}"
                              value="{{this.option1}}"
                            />
                            <span>{{this.option1}}</span>
                          </label>
                        </div>
                        <div>
                          <label>
                            <input
                              type="radio"
                              name="{{this._id}}"
                              value="{{this.option2}}"
                            />
                            <span>{{this.option2}}</span>
                          </label>
                        </div>
                        <div>
                          <label>
                            <input
                              type="radio"
                              name="{{this._id}}"
                              value="{{this.option3}}"
                            />
                            <span>{{this.option3}}</span>
                          </label>
                        </div>
                        <div>
                          <label>
                            <input
                              type="radio"
                              name="{{this._id}}"
                              value="{{this.option4}}"
                            />
                            <span>{{this.option4}}</span>
                          </label>
                        </div>
                      </div>
                    </div>
                    <div class="rightAns">
                      <p>Correct Answer: {{this.correct}}</p>
                    </div>
                  {{/each}}

                </div>
                <hr />
                {{! <a href="/"><button type="submit">Submit</button></a> }}
              </form>
            </div>
          </div>
        </div>
      </div>
    </div>

    {{! <script src="/js/exam.js"></script> }}
    <script>
      {{!-- console.log({{randomItems}}); --}}
    </script>

  </body>
</html>