  <body class="light blog">
<!--Nav markup goes here-->
   {{>navbar}}

<!--End nav-->
<!--Hero-->
<section class="blog_hero">
    <div class="container">
        <div class="text">
            <h1>Jobs</h1>
            <p>All kind of jobs are here</p>
            
        </div>
    </div>

</section>
<section>
    <div class="blog_search">
        <div class="container">
            <div class="container_wrapper">
                <div class="categories">
                    <label for="caterories">Categories</label>
                    <select name="caterories" id="caterories">
                        <option value="popular">Popular Blogs</option>
                        <option value="academic">academic</option>
                        <option value="job">Job Preparation</option>
                        <option value="admission">Admission</option>
                        <option value="skills&it">Skills & IT</option>
                        <option value="carrier">Carrier</option>
                        <option value="freelancing">freelancing</option>
                        <option value="design">design</option>
                        <option value="miscellaneous">Miscellaneous</option>
                    </select>
                </div>
                <div class="search_bar">
                        <!-- SEARCH -->
                    <div class="navigation-search">
                        <input type="search" placeholder="search" class="navigation-search__input" />
                        <svg class="search_icon" xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="navigation-search__icon">
                            <circle cx="11" cy="11" r="8"></circle>
                            <line x1="21" y1="21" x2="16.65" y2="16.65"></line>
                         </svg>
                     </div>
                </div>
            </div>
        </div>
    </div>
</section>

<!-- end of hero segment -->


<!--  splide slider  starts -->
<section id="banner_carousel_1" class="splide" aria-label="Sp  lide Basic HTML Example">
<div class="main_slider">
