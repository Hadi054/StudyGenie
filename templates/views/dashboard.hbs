            {{!-- <form action="/update" method="POST" enctype="multipart/form-data"> --}}
              <input type="file" name="image" id="file-input" style="display: none;">
                {{#if extra.img.data}}
                <img class="dpimg" id="dpimg"
                src="data:{{ extra.img.contentType }};base64,{{ extra.img.data }}"
              />{{else}}
                <img class="dpimg" id="dpimg" src="/images/profile.png"  alt="">
                {{/if}}
                <div class="pro-details">
                  <input class="input-event-username" id="uname" placeholder="Enter Your Username" id="username">
                <h2 class="profile-data-username" id="Puname">{{userinfo.username}}</h2>

              </div>
                  <button id="dp-name-edit" onclick="updateimg()">EDIT</button>
                  <button id="dp-name-update" onclick="updateimgDB()">CONFIRM</button>
              {{!-- </form> --}}
          </div>

            <div class="input-group">
            <div class="profile"  >

              
            <div >
                <label for="First-name">First Name</label>
                <div id="First-name" >
                  <div id="for-update" class="inputPlusEdit">
                    <input class="input-event" id="fname">
                    <p class="wrong">Your email is incorrect or empty</p>
                    <button class="update"  onclick="fnameDB()">CONFIRM</button>
                  </div>
                  <div id="initial">
                    <div class="profile-data" id="ufname">
                    {{#if extra.fname}}
                      {{extra.fname}}
                      {{else}}
                      Not Provided
                      {{/if}}
                    </div>
                    <button class="update"  onclick="updatefname()">&#9998;</button>
                  </div>
                </div>
              </div>
            <div >
                <label for="last name">Last Name</label>
                <div id="Last-name" >
                  <div id="for-update" class="inputPlusEdit">
                    <input class="input-event" id="lname" value="{{extra.lname}}">
                    <button class="update" onclick="lnameDB()">CONFIRM</button>
                  </div>
                  <div id="initial">
                  <div class="profile-data" id="ulname">{{#if extra.lname}}
                    {{extra.lname}}
                    {{else}}
                    Not Provided
                    {{/if}}</div>
                  <button class="update" onclick="updatelname()">&#9998;</button>
                  </div>
                </div>

              </div>
              <div >
                <label for="email">Email</label>
                <div id="Email">
                  <div id="for-update" class="inputPlusEdit">
                  <input class="input-event" id="email" value="{{userinfo.email}}" id="email">
                  <p class="wrong">Your email is incorrect or empty</p>
                  <button class="update" onclick="EmailDB()">CONFIRM</button>
                  </div>
                  <div id="initial">
                  <div class="profile-data" id="uemail">{{userinfo.email}}</div>
                  <button class="update" onclick="updateEmail()">&#9998;</button>
                  </div>
                </div>
              </div>

              <!-- rest form -->
              <div >
                <label for="Class">Class</label>
                <div id="Class">
                  <div id="for-update" class="inputPlusEdit">
                    {{!-- do it here --}}
                    <div class="selectClassDropdown">
                      <select name="classSelection" id="classSelection">
                            <option value="Class 5">Class 5</option>
                          <option value="Class 6">Class 6</option> 
                          <option value="Class 7">Class 7</option>
                          <option value="Class 8">Class 8</option>
                          <option value="SSC">SSC</option>
                          <option value="HSC">HSC</option>
                          <option value="Admission">Admission</option>               
                      </select>
                    </div>
                  {{!-- <input class="input-event" id="class" value="{{extra.class}}" > --}}
                  <button class="update" onclick="ClassDB()">CONFIRM</button>
                  </div>
                  <div id="initial">
                    <div class="profile-data" id="uclass">{{extra.class}}</div>
                  {{!-- <div class="profile-data" id="uclass">{{extra.class}}</div> --}}
                  <button class="update" onclick="updateClass()">&#9998;</button>
                  </div>
                </div>
              </div>
              <div >
                  <label for="department">Department</label>
              <div id="Dept">
                  <div id="for-update" class="inputPlusEdit">
                  <div class="selectClassDropdown">
                      <select name="classSelection" id="classSelection">
                            <option value="Science">Science</option>
                          <option value="Commerce">Commerce</option> 
                          <option value="Arts">Arts</option>
                      </select>
                    </div>                 
                  <button class="update" onclick="DeptDB()">CONFIRM</button>
                  </div>
                  <div id="initial">
                  <div class="profile-data" id="udept">{{extra.dept}}</div>
                  <button class="update" onclick="updatedept()">&#9998;</button>
                  </div>
                </div>
              </div>
              <div >
                {{!-- <label for="password">Confirm Password</label> --}}
                <label for="choices">Chosen Fields</label>
                <div id="Choice">
                  <div id="for-update" class="inputPlusEdit">
                    <div class="selectClassDropdown">
                      <select name="classSelection" id="classSelection">
                          <option value="Medical">Medical</option>
                          <option value="Versity">Versity</option> 
                          <option value="Engineering">Engineering</option>
                          <option value="IBA">IBA</option>              
                      </select>
                    </div>                   
                  <button class="update" onclick="choiceDB()">CONFIRM</button>
                  </div>
                  <div id="initial">
                  <div class="profile-data" id="uchoice">{{extra.choice}}</div>
                  <button class="update" onclick="updatechoice()">&#9998;</button>
                  </div>
                </div>
                <div >
                <label for="Phone">Phone No.</label>
                <div id="Phone">
                  <div id="for-update"  class="inputPlusEdit">
                  <input class="input-event" id="phone" value="{{extra.pnum}}" id="email">
                  <button class="update" onclick="phoneDB()">CONFIRM</button>
                  </div>
                  <div id="initial">
                  <div class="profile-data" id="uphone">{{extra.pnum}}</div>
                  <button class="update" onclick="updatephone()">&#9998;</button>
                  </div>
                </div>
              </div>
              {{!-- update password --}}
               {{!-- <div >
                <label for="Phone">Pass</label>
