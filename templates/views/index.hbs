<!-- newsletter Starts -->

<div class="newsletter">

    <div class="container news_container">
        <div class="shorts">
        <div class="newsletter_shorts">
            <h1 class="title"> Newsletter</h1>
            <div class="news">
            {{#each popularnews}}

            
                <div class="news_item">
                    <div class="news_item_img">
                        <h1 class="heading">{{this.name}}</p>
                        <img src="data:{{ this.img.contentType }};base64,{{ this.img.data }}"  alt="">
                    </div>
                    <div class="news_item_info">
                        <div>
                            <img src="/images/contributor_img/safiul.png
                            " alt="witter_img">
                            <p class="writter">by {{this.Writer}}</p>
                        </div>
                        
                        
                        <p class="date_time">{{this.formattedDate}}</p>
                    </div>
                    <div class="news_item_fullinfo">
                        <p class="post_content">
{{this.short}}                        </p>
                        <button class="showmore btn p-3" onclick="readnews('{{this._id}}')">see more...</button>
                    </div>
    
                </div>
               
            {{/each}}
                        </div>           

        </div>
        <div class="blog_shorts">
            <h1 class="title"> Blogs</h1>
<div class="news">
            {{#each popularblog}}

            
                <div class="news_item">
                    <div class="news_item_img">
                        <h1 class="heading">{{this.name}}</p>
                        <img src="data:{{ this.img.contentType }};base64,{{ this.img.data }}"  alt="">
                    </div>
                    <div class="news_item_info">
                        <div>
                            <img src="/images/contributor_img/safiul.png
                            " alt="witter_img">
                            <p class="writter">by {{this.Writer}}</p>
                        </div>
                        
                        
                        <p class="date_time">{{this.formattedDate}}</p>
                    </div>
                    <div class="news_item_fullinfo">
                        <p class="post_content">
{{this.short}}                        </p>
                        <button class="showmore btn p-3" onclick="readnews('{{this._id}}')">see more...</button>
                    </div>
    
                </div>
               
            {{/each}}
                        </div>           

        </div>
